    "styled-component"
  ],
  "contributors": [
    "@arashmanteghi",
    "https://github.com/marzzy"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evandhq/jupiter-react"
  },
  "bugs": {
    "url": "https://github.com/evandhq/jupiter-react/issues"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-plugin-macros": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hook-form": "^3.23.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-sidenav": "^4.0.3",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build:doc": "react-app-rewired build",
    "test": "react-app-rewired test --silent",
    "test:coverage": "react-app-rewired test --silent --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix",
    "build:lib": "del-cli ./lib && babel src/components -d lib --presets minify && cp -r src/components/iranSharpFont.scss lib/ && cp -r src/components/assets lib/ && cp package.lib.json lib/package.json && cp README.md lib/ && cp CHANGELOG.md lib/ && cp LICENSE lib/",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "storybook": "start-storybook -p 9009 -s public",
    "build:storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "auto-changelog": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-minify": "^0.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react": "^6.24.1",
    "del-cli": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "husky": "^3.0.4",
    "jest-enzyme": "^7.1.1",
    "jest-styled-components": "^6.3.3",
    "node-sass": "^4.13.0",
    "react-app-rewired": "^2.1.3",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1"
  }
}
